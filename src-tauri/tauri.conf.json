{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "icon_space",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "dialog": {
        "all": true,
        "ask": true,
        "confirm": true,
        "message": true,
        "open": true,
        "save": true
      },
      "fs": {
        "all": true,
        "copyFile": true,
        "createDir": true,
        "exists": true,
        "readDir": true,
        "readFile": true,
        "removeDir": true,
        "removeFile": true,
        "renameFile": true,
        "scope": [
          "*",
          "$APP",
          "$APP/*",
          "$CACHE",
          "$CACHE/*",
          "$CONFIG",
          "$CONFIG/*",
          "$LOG",
          "$LOG/*",
          "$HOME/*",
          "$DESKTOP/*"
        ],

        "writeFile": true
      },
      "clipboard": {
        "all": false,
        "readText": false,
        "writeText": true
      },

      "all": true,
      "shell": {
        "all": false,
        "open": true
      },
      "path": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.coolhead.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Icon Space",
        "width": 800,
        "height": 600
      }
    ]
  }
}
